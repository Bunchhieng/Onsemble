    <link rel="icon" type="image/png" href="/assets/img/Onsemble.png">
    <title>
        <% if (title) { %>
        <%= title %>
        <% } %>
    </title>
    <!-- Bootstrap CSS-->
    <link href="/assets/css/bootstrap.css" rel="stylesheet">
    <!-- Ion Icon Styles CSS-->
    <link href="/assets/css/ionicons.css" rel="stylesheet">
    <!-- Font Awesome CSS-->
    <link href="/assets/css/font-awesome.css" rel="stylesheet">
    <!-- Fancy Box CSS-->
    <link href="/assets/css/jquery.fancybox.css" rel="stylesheet">
    <link rel="stylesheet"
          href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.23/themes/south-street/jquery-ui.css">
    <!-- Viewpoint Animation CSS-->
    <link href="/assets/css/animations.min.css" rel="stylesheet">
    <!-- Custom CSS-->
    <link href="/assets/css/main.css" rel="stylesheet">
    <link href="/assets/css/stage.css" rel="stylesheet" type="text/css"/>
</head>

<body data-spy="scroll" data-target="#menu-section">
<!-- Navigation Bar-->
<div id="menu-section" class="navbar navbar-inverse navbar-fixed-top scroll-me">
    <div class="container">
        <div class="navbar-header">
            <button type="button" data-toggle="collapse" data-target=".navbar-collapse" class="navbar-toggle"><span
                        class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span></button>
            <a href="/" class="pull-left"><img src="/assets/img/Onsemble-Light.png" class="navbar-logo"></a>
            <p class="nav navbar-text nav-logo"><span style="color: #439756">n</span>semble</p>
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav navbar-right">
                <li><a href="/upload">UPLOAD</a></li>
                <li><a href="/discover/">DISCOVER</a></li>
                <li><a href="/stage/">STAGE</a></li>
                <li><a href="/logout/">LOG OUT</a></li>
            </ul>
        </div>
    </div>
</div>


<div id="stage">
    <!-- Home End-->
    <section id="stage-profile" class="container-fluid">
        <div class="col-md-12">
            <div class="profile-img-container">
                <img id="pro-pic" src="/assets/img/team/santi.jpg"
                     class="img-thumbnail img-responsive profile-pic "/>      
        </div>
        <div class="profile-info user-name  text-center col-md-12">
            Santiago Paredes
        </div>
        <div class="profile-info text-center col-md-12">
         Lowell, MA
        </div>
        <div class="profile-info text-center col-md-12">
         Followers: <% if (user) { %>
                    <%= user[0].profile.followers %>
                    <% } %>
        </div>
        <div class="profile-info text-center col-md-12">
         Following: <% if (user) { %>
                    <%= user[0].profile.followings %>
                    <% } %>
        </div>
        <div class="user-description col-md-12">
        <div class="text-center title-box">
            I'm a student at UMASS Lowell. My interests have manifested themselves in a number of unique ways and, as a result.
        </div>
        </div>
        <div class="col-md-12">
            <div class="scroll-down text-center">
                            <a class="btn btn-circle"href="#releases">
                            <i class="fa fa-angle-double-down animated"></i></a>
                         </div> 
                         </div>
    </section>
    <section id="releases" class="container-fluid">
        <div>
            <div class="text-center">
                <h3 id="recent-releases" class="font-black"><a class="recent">Recent Releases</a></h3>
                <div class="container">
                    <hr>
                </div>
                <% for(var i = 0; i < 3; i++) { %>
                <div class="col-lg-4 col-md-4">
                    <iframe src="<%= user[0].profile.videos[i] %>" width="400" height="300" frameborder="0"
                            allowfullscreen=""></iframe>
                </div>
                <% } %>
            </div>
        </div>
    </section>
    <div class="container-fluid bg-blk">
        <div class="col-sm-8 col-sm-offset-2 col-md-8 col-md-offset-2 col-lg-8 col-lg-offset-2 text-center">
            <div class="social"><a href="#" class="btn button-onsemble btn-onsemble"><i class="fa fa-facebook"></i></a>
                <a href="#" class="btn button-onsemble btn-onsemble"><i class="fa fa-twitter"></i></a> <a href="#"
                                                                                                          class="btn butemble"><i
                            class="fa fa-google-plus"></i></a> <a href="#" class="btn button-onsemble btn-onsemble"><i
                            class="fa fa-github"></i></a></div>
        </div>
    </div>
</div>
</div>
<script src="/assets/js/jquery-1.11.1.js"></script>
