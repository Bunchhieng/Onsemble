      <!-- upload Section -->
    <section id="upload">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Upload video</h2>
                    <h3 class="section-subheading text-muted onsemble-green col-md-6 col-md-offset-3">Copy and paste your YouTube link here.</h3>
                </div>
            </div>
            <div id="upload" class="row">
                <div class="col-lg-12">
                    <form name="sentMessage" id="uploadForm" novalidate>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Your Name *" id="name" required data-validation-required-message="Please enter your name.">
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    <input type="account" class="form-control" placeholder="Your YouTube Account *" id="account" required data-validation-required-message="Please enter your YouTube account.">
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    <input type="link" class="form-control" placeholder="Your YouTube Link *" id="link" required data-validation-required-message="Please enter your YouTube link.">
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <textarea class="form-control" placeholder="Tell Us Something About Your Video *" id="message" required data-validation-required-message="Please enter a message."></textarea>
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-lg-12 text-center">
                                <div id="success"></div>
                                <button type="submit" class="btn button-onsemble btn-onsemble" style="color:#fff">Submit</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>
</body>

</html>
